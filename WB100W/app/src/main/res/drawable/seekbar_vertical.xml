<?xml version="1.0" encoding="UTF-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <gradient android:startColor="#fffdfcef" android:endColor="#fffdfcef" android:angle="270.0" android:centerY="0.75" android:centerColor="#fffdfcef" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress" >
        <clip android:drawable="@drawable/icon_volume"/>
    </item>
</layer-list>